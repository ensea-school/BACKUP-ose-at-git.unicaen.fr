<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paiement\Entity\Db\TblPaiement" table="TBL_PAIEMENT">
    <unique-constraints>
      <unique-constraint name="tbl_paiement__un" columns="SERVICE_A_PAYER_ID,INTERVENANT_ID,MISE_EN_PAIEMENT_ID,MISSION_ID"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
    </id>
    <field name="heuresAPayerAA" type="float" column="HEURES_A_PAYER_AA" precision="126" scale="0" nullable="false" />
    <field name="heuresAPayerAC" type="float" column="HEURES_A_PAYER_AC" precision="126" scale="0" nullable="false" />
    <field name="heuresDemandeesAA" type="float" column="HEURES_DEMANDEES_AA" precision="126" scale="0" nullable="false" />
    <field name="heuresDemandeesAC" type="float" column="HEURES_DEMANDEES_AC" precision="126" scale="0" nullable="false" />
    <field name="heuresPayeesAA" type="float" column="HEURES_PAYEES_AA" precision="126" scale="0" nullable="false" />
    <field name="heuresPayeesAC" type="float" column="HEURES_PAYEES_AC" precision="126" scale="0" nullable="false" />
    <many-to-one field="formuleResultatService" target-entity="Application\Entity\Db\FormuleResultatService">
        <join-column name="FORMULE_RES_SERVICE_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="formuleResultatServiceReferentiel" target-entity="Application\Entity\Db\FormuleResultatServiceReferentiel">
        <join-column name="FORMULE_RES_SERVICE_REF_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="mission" target-entity="Mission\Entity\Db\Mission">
      <join-column name="MISSION_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="typeHeures" target-entity="OffreFormation\Entity\Db\TypeHeures">
      <join-column name="TYPE_HEURES_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="structure" target-entity="Lieu\Entity\Db\Structure" fetch="LAZY">
        <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="periodePaiement" target-entity="Application\Entity\Db\Periode" fetch="LAZY">
        <join-column name="PERIODE_PAIEMENT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="miseEnPaiement" target-entity="Paiement\Entity\Db\MiseEnPaiement" fetch="LAZY">
        <join-column name="MISE_EN_PAIEMENT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="centreCout" target-entity="Paiement\Entity\Db\CentreCout" fetch="LAZY">
      <join-column name="CENTRE_COUT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant" fetch="LAZY">
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="annee" target-entity="Application\Entity\Db\Annee" fetch="LAZY">
        <join-column name="ANNEE_ID" referenced-column-name="ID"/>
    </many-to-one>
  </entity>
</doctrine-mapping>
