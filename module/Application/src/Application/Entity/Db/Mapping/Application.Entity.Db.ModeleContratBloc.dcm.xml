<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Db\ModeleContratBloc" table="MODELE_CONTRAT_BLOC">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="nom" type="string" column="NOM" nullable="false"/>
        <field name="requete" type="string" column="REQUETE" nullable="false"/>
        <many-to-one field="modeleContrat" target-entity="Application\Entity\Db\ModeleContrat">
            <join-columns>
                <join-column name="MODELE_CONTRAT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>
