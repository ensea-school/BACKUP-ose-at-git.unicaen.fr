<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Workflow\Entity\Db\WorkflowEtapeDependance" table="WORKFLOW_ETAPE_DEPENDANCE">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <many-to-one field="etapeSuivante" target-entity="Workflow\Entity\Db\WorkflowEtape">
            <join-column name="ETAPE_SUIVANTE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="etapePrecedante" target-entity="Workflow\Entity\Db\WorkflowEtape">
            <join-column name="ETAPE_PRECEDANTE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <field name="active" type="boolean" column="ACTIVE"/>
        <many-to-one field="typeIntervenant" target-entity="Intervenant\Entity\Db\TypeIntervenant">
            <join-column name="TYPE_INTERVENANT_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="perimetre" target-entity="Application\Entity\Db\Perimetre">
            <join-column name="PERIMETRE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <field name="avancement" type="integer" column="AVANCEMENT"/>
    </entity>
</doctrine-mapping>