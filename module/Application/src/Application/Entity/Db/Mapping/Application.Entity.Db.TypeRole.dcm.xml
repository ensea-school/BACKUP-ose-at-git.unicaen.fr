<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\TypeRole" table="TYPE_ROLE">
    <id name="id" type="string" column="ID" length="2">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoCreateur" type="integer" column="HISTO_CREATEUR" nullable="false"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoDestructeur" type="integer" column="HISTO_DESTRUCTEUR" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="false"/>
    <field name="histoModificateur" type="integer" column="HISTO_MODIFICATEUR" nullable="false"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="libelle" type="string" column="LIBELLE" length="30" nullable="false"/>
    <many-to-many field="typeStructure" target-entity="Application\Entity\Db\TypeStructure" mapped-by="typeRole"/>
  </entity>
</doctrine-mapping>
