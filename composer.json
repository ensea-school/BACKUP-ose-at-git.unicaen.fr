{
    "name"             : "unicaen/ose",
    "version"          : "1.0.0",
    "description"      : "Organisation des Services d'Enseignement (OSE)",
    "repositories"     : [
        {
            "type": "composer",
            "url" : "https://gest.unicaen.fr/packagist"
        }
    ],
    "require"          : {
        "php"                                         : ">=8.2",
        "unicaen/app"                                 : "6.1.4",
        "unicaen/authentification"                    : "6.1.0",
        "unicaen/privilege"                           : "6.1.2",
        "unicaen/utilisateur"                         : "6.1.3",
        "laminas/laminas-file"                        : "^2.8",
        "unicaen/code"                                : "6.2.1",
        "unicaen/import"                              : "6.0.3",
        "unicaen/tbl"                                 : "6.2.3",
        "unicaen/open-document"                       : "6.0.10",
        "unicaen/siham"                               : "6.1.6",
        "unicaen/mail"                                : "6.1.5",
        "unicaen/vue"                                 : "6.2.4",
        "unicaen/bddadmin"                            : "dev-modernisation-gestion-donnees",
        "laminas/laminas-cache-storage-adapter-memory": "2.3.0",
        "laminas/laminas-text"                        : "2.11",
        "laminas/laminas-cli"                         : "1.10.0",
        "symfony/process"                             : "7.1.5",
        "connecteur-ose/actul": "dev-master",
        "connecteur-ose/pegase": "dev-master"
    },
    "require-dev"      : {
        "squizlabs/php_codesniffer"         : "*",
        "phpcompatibility/php-compatibility": "*",
        "phan/phan"                         : "^5.3",
        "phpmd/phpmd"                       : "*",
        "phpunit/phpunit"                   : "^9"
    },
    "prefer-stable"    : true,
    "config"           : {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin"             : true,
            "composer/package-versions-deprecated"          : true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "laminas-api-tools/api-tools-asset-manager"     : true
        }
    },
    "scripts"          : {
        "sniffer:php8"    : "./vendor/bin/phpcs -p ./module --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 8.0",
        "phpcs"           : "./vendor/bin/phpcs -p ./module --standard=PSR12",
        "post-install-cmd": "php bin/ose install"
    },
    "minimum-stability": "dev"
}