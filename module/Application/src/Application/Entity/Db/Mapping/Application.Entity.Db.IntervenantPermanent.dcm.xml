<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\IntervenantPermanent" table="INTERVENANT_PERMANENT">
<!--    <indexes>
      <index name="IDX_FB425D27E774C1C4" columns="CORPS_ID"/>
      <index name="IDX_FB425D27DF6D9E85" columns="UNITE_RECHERCHE_ID"/>
    </indexes>-->
<!--    <unique-constraints>
      <unique-constraint name="intervenant_permanent_idx" columns="INTERVENANT_ID"/>
    </unique-constraints>-->
<!--    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>-->
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <many-to-one field="corps" target-entity="Application\Entity\Db\Corps">
      <join-columns>
        <join-column name="CORPS_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sectionCnu" target-entity="Application\Entity\Db\SectionCnu">
      <join-columns>
        <join-column name="SECTION_CNU_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
