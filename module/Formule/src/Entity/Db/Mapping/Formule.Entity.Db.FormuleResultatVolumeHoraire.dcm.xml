    <field name="tauxServiceDu" type="float" column="TAUX_SERVICE_DU" precision="126" scale="0" nullable="false"/>
    <field name="tauxServiceCompl" type="float" column="TAUX_SERVICE_COMPL" precision="126" scale="0" nullable="false"/>
    <field name="ponderationServiceDu" type="float" column="PONDERATION_SERVICE_DU" precision="126" scale="0" nullable="false"/>
    <field name="ponderationServiceCompl" type="float" column="PONDERATION_SERVICE_COMPL" precision="126" scale="0" nullable="false"/>
    <field name="heures" type="float" column="HEURES" precision="126" scale="0" nullable="false"/>

    <field name="param1" type="string" column="PARAM_1" length="100" nullable="true"/>
    <field name="param2" type="string" column="PARAM_2" length="100" nullable="true"/>
    <field name="param3" type="string" column="PARAM_3" length="100" nullable="true"/>
    <field name="param4" type="string" column="PARAM_4" length="100" nullable="true"/>
    <field name="param5" type="string" column="PARAM_5" length="100" nullable="true"/>

    <field name="heuresServiceFi" type="float" column="HEURES_SERVICE_FI" precision="126" scale="0" nullable="false"/>
    <field name="heuresServiceFa" type="float" column="HEURES_SERVICE_FA" precision="126" scale="0" nullable="false"/>
    <field name="heuresServiceFc" type="float" column="HEURES_SERVICE_FC" precision="126" scale="0" nullable="false"/>
    <field name="heuresServiceReferentiel" type="float" column="HEURES_SERVICE_REFERENTIEL" precision="126" scale="0" nullable="false"/>

    <field name="heuresNonPayableFi" type="float" column="HEURES_NON_PAYABLE_FI" precision="126" scale="0" nullable="false"/>
