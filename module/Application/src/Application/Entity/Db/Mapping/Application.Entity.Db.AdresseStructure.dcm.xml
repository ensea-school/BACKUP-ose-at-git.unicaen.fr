<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\AdresseStructure" table="ADRESSE_STRUCTURE">
    <indexes>
      <index name="IDX_D72AAFC87F29375B" columns="HISTORIQUE_ID"/>
      <index name="IDX_D72AAFC8884B0F7B" columns="STRUCTURE_ID"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="codePostal" type="string" column="CODE_POSTAL" length="15" nullable="true"/>
    <field name="localite" type="string" column="LOCALITE" length="26" nullable="true"/>
    <field name="nomVoie" type="string" column="NOM_VOIE" length="22" nullable="true"/>
    <field name="noVoie" type="string" column="NO_VOIE" length="10" nullable="true"/>
    <field name="paysCodeInsee" type="string" column="PAYS_CODE_INSEE" length="3" nullable="true"/>
    <field name="paysLibelle" type="string" column="PAYS_LIBELLE" length="30" nullable="true"/>
    <field name="principale" type="boolean" column="PRINCIPALE" nullable="true"/>
    <field name="telephone" type="string" column="TELEPHONE" length="20" nullable="false"/>
    <field name="ville" type="string" column="VILLE" length="26" nullable="true"/>
    <many-to-one field="structure" target-entity="Application\Entity\Db\Structure">
      <join-columns>
        <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="historique" target-entity="Application\Entity\Db\Historique">
      <join-columns>
        <join-column name="HISTORIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
