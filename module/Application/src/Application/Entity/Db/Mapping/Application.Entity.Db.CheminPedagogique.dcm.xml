<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\CheminPedagogique" table="CHEMIN_PEDAGOGIQUE">
    <indexes>
      <index name="IDX_B1F283FEB4F27801" columns="ETAPE_ID"/>
      <index name="IDX_B1F283FE7F29375B" columns="HISTORIQUE_ID"/>
      <index name="IDX_B1F283FEC0569A10" columns="PERIODE_ID"/>
      <index name="IDX_B1F283FEE9B3446E" columns="SOURCE_ID"/>
      <index name="IDX_B1F283FE8AB4A516" columns="ELEMENT_PEDAGOGIQUE_ID"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="chemin_pedago_src_id_un" columns="SOURCE_CODE"/>
    </unique-constraints>
    <id name="dateDebutAnneeUniv" type="datetime" column="DATE_DEBUT_ANNEE_UNIV"/>
    <id name="etape" association-key="true"/>
    <id name="elementPedagogique" association-key="true"/>
    <field name="sourceCode" type="string" column="SOURCE_CODE" length="20" nullable="true"/>
    <field name="ue" type="string" column="UE" length="60" nullable="true"/>
    <field name="index" type="integer" column="index" nullable="false"/>
    <one-to-one field="etape" target-entity="Application\Entity\Db\Etape">
      <join-columns>
        <join-column name="ETAPE_ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="elementPedagogique" target-entity="Application\Entity\Db\ElementPedagogique">
      <join-columns>
        <join-column name="ELEMENT_PEDAGOGIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="source" target-entity="Application\Entity\Db\Source">
      <join-columns>
        <join-column name="SOURCE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="periode" target-entity="Application\Entity\Db\Periode">
      <join-columns>
        <join-column name="PERIODE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="historique" target-entity="Application\Entity\Db\Historique">
      <join-columns>
        <join-column name="HISTORIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
