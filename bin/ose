#!/usr/bin/env php
<?php

spl_autoload_register(function ($class) {
    $dir = dirname(__DIR__) . '/admin/src/';
    $class = $dir . str_replace('\\', '/', $class) . '.php';

    if (file_exists($class)) {
        require_once $class;
    }
});

$c  = new Console();
$oa = new OseAdmin($c);
$oa->init();

$oa->run($c->getArg(1) ?: 'help');