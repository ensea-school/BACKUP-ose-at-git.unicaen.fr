<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\PieceJointe" table="PIECE_JOINTE">
    <indexes>
      <index name="IDX_F1DE615859860CDC" columns="HISTO_CREATEUR_ID"/>
      <index name="IDX_F1DE61587E9E2204" columns="HISTO_DESTRUCTEUR_ID"/>
      <index name="IDX_F1DE615863F06898" columns="HISTO_MODIFICATEUR_ID"/>
      <index name="IDX_F1DE615852CE5789" columns="DOSSIER_ID"/>
