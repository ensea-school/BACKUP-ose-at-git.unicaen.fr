<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\FormuleVolumeHoraireReferentiel" table="V_FORMULE_VOLUME_HORAIRE_REF" read-only="true">
    <id name="id" type="integer" column="ID" />
    <many-to-one field="serviceReferentiel" target-entity="Application\Entity\Db\ServiceReferentiel">
      <join-columns>
        <join-column name="SERVICE_REFERENTIEL_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant">
      <join-columns>
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="typeVolumeHoraire" target-entity="Application\Entity\Db\TypeVolumeHoraire">
      <join-columns>
        <join-column name="TYPE_VOLUME_HORAIRE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="etatVolumeHoraire" target-entity="Application\Entity\Db\EtatVolumeHoraire">
      <join-columns>
        <join-column name="ETAT_VOLUME_HORAIRE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <field name="heures" type="float" column="HEURES" precision="126" scale="0" nullable="false"/>

    <many-to-one field="formuleServiceReferentiel" target-entity="Application\Entity\Db\FormuleServiceReferentiel">
      <join-columns>
        <join-column name="SERVICE_REFERENTIEL_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <one-to-one field="volumeHoraireReferentiel" target-entity="Application\Entity\Db\VolumeHoraireReferentiel">
      <join-columns>
        <join-column name="ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
  </entity>
</doctrine-mapping>
