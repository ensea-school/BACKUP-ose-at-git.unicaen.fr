<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\VServiceValide" table="V_SERVICE_VALIDE">
    
    <id name="id" type="integer" column="ID"/>
    
    <field name="libelle" type="string" column="LIBELLE" length="120" nullable="false"/>
    <field name="heures" type="float" column="HEURES" precision="126" scale="0" nullable="false"/>
    <field name="code" type="string" column="CODE" length="25" nullable="false"/>
    
    <many-to-one field="elementPedagogique" target-entity="Application\Entity\Db\ElementPedagogique">
      <join-columns>
        <join-column name="ELEMENT_PEDAGOGIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant">
      <join-columns>
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="service" target-entity="Application\Entity\Db\Service">
      <join-columns>
        <join-column name="SERVICE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="validation" target-entity="Application\Entity\Db\Validation">
      <join-columns>
        <join-column name="VALIDATION_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="volumeHoraire" target-entity="Application\Entity\Db\VolumeHoraire">
      <join-columns>
        <join-column name="VOLUME_HORAIRE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    
  </entity>
</doctrine-mapping>
