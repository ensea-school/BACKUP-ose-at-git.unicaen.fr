<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\PieceJointe" table="PIECE_JOINTE">
    <indexes>
      <index name="IDX_F1DE61587F29375B" columns="HISTORIQUE_ID"/>
      <index name="IDX_F1DE615878FF2BCB" columns="INTERVENANT_ID"/>
      <index name="IDX_F1DE6158C2443469" columns="TYPE_ID"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="nom" type="string" column="NOM" length="250" nullable="false"/>
    <many-to-one field="type" target-entity="Application\Entity\Db\TypePieceJointe">
      <join-columns>
        <join-column name="TYPE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant">
      <join-columns>
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="historique" target-entity="Application\Entity\Db\Historique">
      <join-columns>
        <join-column name="HISTORIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
