<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\DiplomeEtape" table="DIPLOME_ETAPE">
    <indexes>
      <index name="IDX_62C3B1B281526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_62C3B1B2152A023D" columns="DIPLOME_ID"/>
      <index name="IDX_62C3B1B2F791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_62C3B1B2B4F27801" columns="ETAPE_ID"/>
      <index name="IDX_62C3B1B2862159D1" columns="HISTO_MODIFICATEUR"/>
      <index name="IDX_62C3B1B2E9B3446E" columns="SOURCE_ID"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="diplome_etape_src_id_un" columns="SOURCE_CODE"/>
    </unique-constraints>
    <id name="diplome" association-key="true"/>
    <id name="etape" association-key="true"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="sourceCode" type="string" column="SOURCE_CODE" length="20" nullable="true"/>
    <field name="index" type="integer" column="index" nullable="false"/>
    <one-to-one field="etape" target-entity="Application\Entity\Db\Etape">
      <join-columns>
        <join-column name="ETAPE_ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
    <one-to-one field="diplome" target-entity="Application\Entity\Db\Diplome">
      <join-columns>
        <join-column name="DIPLOME_ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="source" target-entity="Application\Entity\Db\Source">
      <join-columns>
        <join-column name="SOURCE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
