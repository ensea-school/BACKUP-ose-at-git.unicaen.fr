<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\ElementSection" table="ELEMENT_SECTION">
    <indexes>
      <index name="IDX_40031C4E59860CDC" columns="HISTO_CREATEUR_ID"/>
      <index name="IDX_40031C4E7E9E2204" columns="HISTO_DESTRUCTEUR_ID"/>
      <index name="IDX_40031C4E63F06898" columns="HISTO_MODIFICATEUR_ID"/>
      <index name="IDX_40031C4EE9B3446E" columns="SOURCE_ID"/>
      <index name="IDX_40031C4E8AB4A516" columns="ELEMENT_PEDAGOGIQUE_ID"/>
      <index name="IDX_40031C4E8AD1AE90" columns="SECTION_CNU_ID"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="element_section__unv1" columns="SOURCE_CODE"/>
      <unique-constraint name="element_section__un" columns="ELEMENT_PEDAGOGIQUE_ID,SECTION_CNU_ID"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoCreation" type="datetime" column="HISTO_CREATION" nullable="false"/>
    <field name="histoDestruction" type="datetime" column="HISTO_DESTRUCTION" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="sourceCode" type="string" column="SOURCE_CODE" length="20" nullable="true"/>
    <field name="validiteDebut" type="datetime" column="VALIDITE_DEBUT" nullable="false"/>
    <field name="validiteFin" type="datetime" column="VALIDITE_FIN" nullable="true"/>
    <many-to-one field="elementPedagogique" target-entity="Application\Entity\Db\ElementPedagogique">
      <join-columns>
        <join-column name="ELEMENT_PEDAGOGIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="sectionCnu" target-entity="Application\Entity\Db\SectionCnu">
      <join-columns>
        <join-column name="SECTION_CNU_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="source" target-entity="Application\Entity\Db\Source">
      <join-columns>
        <join-column name="SOURCE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="HISTO_CREATEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
