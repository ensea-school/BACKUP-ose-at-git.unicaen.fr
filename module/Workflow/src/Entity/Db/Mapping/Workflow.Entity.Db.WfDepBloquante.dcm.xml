<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Workflow\Entity\Db\WfDepBloquante" table="WF_DEP_BLOQUANTE">
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="wfEtapeDep" target-entity="Workflow\Entity\Db\WfEtapeDep">
      <join-columns>
        <join-column name="WF_ETAPE_DEP_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="tblWorkflow" target-entity="Workflow\Entity\Db\TblWorkflow">
      <join-columns>
        <join-column name="TBL_WORKFLOW_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>