<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\MotifNonPaiement" table="MOTIF_NON_PAIEMENT">
    <indexes>
      <index name="IDX_212E995D81526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_212E995DF791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_212E995D862159D1" columns="HISTO_MODIFICATEUR"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="libelleCourt" type="string" column="LIBELLE_COURT" length="50" nullable="false"/>
    <field name="libelleLong" type="string" column="LIBELLE_LONG" length="200" nullable="false"/>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
