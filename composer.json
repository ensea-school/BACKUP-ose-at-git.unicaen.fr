{
    "name"             : "unicaen/ose",
    "description"      : "Organisation des Services d'Enseignement (OSE)",
    "repositories"     : [
        {
            "type": "composer",
            "url" : "https://gest.unicaen.fr/packagist"
        }
    ],
    "require"          : {
        "php"                                         : ">=8.2",
        "unicaen/app"                                 : "6.1.1",
        "unicaen/authentification"                    : "dev-php82 as 6.0.1",
        "unicaen/privilege"                           : "6.0.1",
        "unicaen/utilisateur"                         : "dev-php82",
        "laminas/laminas-file"                        : "^2.8",
        "unicaen/code"                                : "dev-php82",
        "unicaen/import"                              : "6.0.1",
        "unicaen/tbl"                                 : "dev-php82",
        "unicaen/open-document"                       : "6.0.2",
        "unicaen/siham"                               : "6.1.2",
        "unicaen/mail"                                : "dev-php82",
        "unicaen/vue"                                 : "dev-php82",
        "laminas/laminas-cache-storage-adapter-memory": "2.2.0",
        "laminas/laminas-text"                        : "2.10",
        "unicaen/console"                             : "dev-php82"
    },
    "require-dev"      : {
        "squizlabs/php_codesniffer"         : "*",
        "phpcompatibility/php-compatibility": "*",
        "phan/phan"                         : "^5.3",
        "phpmd/phpmd"                       : "*",
        "phpunit/phpunit"                   : "^9"
    },
    "prefer-stable"    : true,
    "config"           : {
        "allow-plugins": {
            "laminas/laminas-dependency-plugin"             : true,
            "composer/package-versions-deprecated"          : true,
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "laminas-api-tools/api-tools-asset-manager"     : true
        }
    },
    "scripts"          : {
        "sniffer:php8": "./vendor/bin/phpcs -p ./module --standard=vendor/phpcompatibility/php-compatibility/PHPCompatibility --runtime-set testVersion 8.0",
        "phpcs"       : "./vendor/bin/phpcs -p ./module --standard=PSR12"
    },
    "minimum-stability": "dev"
}
