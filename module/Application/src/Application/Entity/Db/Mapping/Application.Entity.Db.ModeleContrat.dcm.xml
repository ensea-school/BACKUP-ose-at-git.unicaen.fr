<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Db\ModeleContrat" table="MODELE_CONTRAT">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="libelle" type="string" column="LIBELLE" nullable="false"/>
        <field name="contenu" type="blob" column="CONTENU" nullable="false"/>
        <field name="requete" type="string" column="REQUETE" nullable="false"/>
        <many-to-one field="statutIntervenant" target-entity="Application\Entity\Db\StatutIntervenant">
            <join-columns>
                <join-column name="STATUT_INTERVENANT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="structure" target-entity="Application\Entity\Db\Structure">
            <join-columns>
                <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <one-to-many field="bloc" target-entity="Application\Entity\Db\ModeleContratBloc" mapped-by="modeleContrat"/>
    </entity>
</doctrine-mapping>
