<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="PieceJointe\Entity\Db\TblPieceJointe" table="TBL_PIECE_JOINTE">
        <indexes>
            <index name="IDX_5D1326F5AA401F5C" columns="ANNEE_ID"/>
            <index name="IDX_5D1326F578FF2BCB" columns="INTERVENANT_ID"/>
            <index name="IDX_5D1326F5F202337" columns="TYPE_PIECE_JOINTE_ID"/>
        </indexes>
        <unique-constraints>
            <unique-constraint name="tbl_piece_jointe__un" columns="TYPE_PIECE_JOINTE_ID,INTERVENANT_ID"/>
        </unique-constraints>
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
            <sequence-generator sequence-name="TBL_PIECE_JOINTE_ID_seq" allocation-size="1" initial-value="1"/>
        </id>
        <field name="demandee" type="boolean" column="DEMANDEE" nullable="false">
            <options>
                <option name="default">0</option>
            </options>
        </field>
        <field name="fournie" type="boolean" column="FOURNIE" nullable="false">
            <options>
                <option name="default">0</option>
            </options>
        </field>
        <field name="validee" type="boolean" column="VALIDEE" nullable="false">
            <options>
                <option name="default">0</option>
            </options>
        </field>
        <field name="heuresPourSeuil" type="float" column="HEURES_POUR_SEUIL" nullable="false"/>
        <many-to-one field="typePieceJointe" target-entity="PieceJointe\Entity\Db\TypePieceJointe" fetch="LAZY">
            <join-columns>
                <join-column name="TYPE_PIECE_JOINTE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="pieceJointe" target-entity="PieceJointe\Entity\Db\PieceJointe" fetch="LAZY">
            <join-columns>
                <join-column name="PIECE_JOINTE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="intervenant" target-entity="Intervenant\Entity\Db\Intervenant" fetch="LAZY">
            <join-columns>
                <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="annee" target-entity="Application\Entity\Db\Annee" fetch="LAZY">
            <join-columns>
                <join-column name="ANNEE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <field name="obligatoire" type="boolean" column="OBLIGATOIRE" nullable="false">
            <options>
                <option name="default">1</option>
            </options>
        </field>
        <field name="demandeApresRecrutement" type="boolean" column="DEMANDEE_APRES_RECRUTEMENT" nullable="false">
            <options>
                <option name="default">0</option>
            </options>
        </field>
    </entity>
</doctrine-mapping>
