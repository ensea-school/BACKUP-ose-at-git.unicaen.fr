<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Emploi" table="EMPLOI">
    <indexes>
      <index name="IDX_8458777C20B9A217" columns="INTERVENANT_EXTERIEUR_ID"/>
      <index name="IDX_8458777CF0035C1C" columns="EMPLOYEUR_ID"/>
      <index name="IDX_8458777C81526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_8458777CF791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_8458777C862159D1" columns="HISTO_MODIFICATEUR"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="emploi_un" columns="INTERVENANT_ID,EMPLOYEUR_ID,DATE_DEBUT"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="dateDebut" type="datetime" column="DATE_DEBUT" nullable="false"/>
    <field name="dateFin" type="datetime" column="DATE_FIN" nullable="true"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="intervenantId" type="integer" column="INTERVENANT_ID" nullable="false"/>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="employeur" target-entity="Application\Entity\Db\Employeur">
      <join-columns>
        <join-column name="EMPLOYEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenantExterieur" target-entity="Application\Entity\Db\IntervenantExterieur">
      <join-columns>
        <join-column name="INTERVENANT_EXTERIEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
