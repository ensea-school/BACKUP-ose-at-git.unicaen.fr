<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Personnel" table="PERSONNEL">
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoCreateur" type="integer" column="HISTO_CREATEUR" nullable="false"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoDestructeur" type="integer" column="HISTO_DESTRUCTEUR" nullable="true"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModificateur" type="integer" column="HISTO_MODIFICATEUR" nullable="false"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="login" type="string" column="LOGIN" length="50" nullable="false"/>
    <field name="nom" type="string" column="NOM" length="60" nullable="false"/>
    <field name="personnelId" type="integer" column="PERSONNEL_ID" nullable="false"/>
    <field name="prenom" type="string" column="PRENOM" length="60" nullable="false"/>
  </entity>
</doctrine-mapping>
