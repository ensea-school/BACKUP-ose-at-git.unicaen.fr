<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Application\Entity\Db\WfEtapeDep" table="WF_ETAPE_DEP">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="active" type="boolean" column="ACTIVE" nullable="false"/>
        <field name="locale" type="boolean" column="LOCALE" nullable="false"/>
        <field name="integrale" type="boolean" column="INTEGRALE" nullable="false"/>
        <field name="partielle" type="boolean" column="PARTIELLE" nullable="false"/>
        <field name="obligatoire" type="boolean" column="OBLIGATOIRE" nullable="false"/>
        <many-to-one field="etapeSuiv" target-entity="Application\Entity\Db\WfEtape">
            <join-columns>
                <join-column name="ETAPE_SUIV_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="etapePrec" target-entity="Application\Entity\Db\WfEtape">
            <join-columns>
                <join-column name="ETAPE_PREC_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="typeIntervenant" target-entity="Intervenant\Entity\Db\TypeIntervenant">
            <join-columns>
                <join-column name="TYPE_INTERVENANT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>