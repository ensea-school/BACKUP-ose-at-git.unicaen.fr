{
    "name"        : "unicaen/ose",
    "description" : "Organisation des Services d'Enseignement (OSE)",
    "repositories": [
        {
            "type": "composer",
            "url" : "https://gest.unicaen.fr/packagist"
        }
    ],
    "require"     : {
        "php"                    : ">=7.4",
        "unicaen/app"            : "3.1.22",
        "unicaen/auth"           : "3.2.5",
        "unicaen/bjy-authorize"  : "3.0.4",
        "unicaen/code"           : "4.5",
        "unicaen/import"         : "3.1.6",
        "unicaen/tbl"            : "4.1",
        "unicaen/open-document"  : "3.0.2"
    },
    "autoload": {
        "psr-4": {
            "Application\\": "module/Application/src/",
            "Plafond\\": "module/Plafond/src/",
            "Indicateur\\": "module/Indicateur/src/"
        }
    },
    "require-dev" : {
        "zendframework/zend-test"           : "3.3.0",
        "unicaen/php-compat"                : "dev-master"
    }
}
