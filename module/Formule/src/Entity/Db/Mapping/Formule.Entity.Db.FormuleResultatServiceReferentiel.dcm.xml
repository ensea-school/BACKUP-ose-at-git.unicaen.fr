<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Formule\Entity\Db\FormuleResultatServiceReferentiel" table="FORMULE_RESULTAT_SERVICE_REF" read-only="true">
    <id name="id" type="integer" column="ID" />
    <many-to-one field="formuleResultat" target-entity="Formule\Entity\Db\FormuleResultat">
      <join-columns>
        <join-column name="FORMULE_RESULTAT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="serviceReferentiel" target-entity="Referentiel\Entity\Db\ServiceReferentiel">
      <join-columns>
        <join-column name="SERVICE_REFERENTIEL_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <one-to-many field="miseEnPaiement" target-entity="Paiement\Entity\Db\MiseEnPaiement" mapped-by="formuleResultatServiceReferentiel" />
    <many-to-many field="centreCout" target-entity="Paiement\Entity\Db\CentreCout">
        <join-table name="V_FR_SERVICE_REF_CENTRE_COUT">
            <join-columns>
                <join-column name="FORMULE_RESULTAT_SERV_REF_ID" referenced-column-name="ID" unique="true"/>
            </join-columns>
            <inverse-join-columns>
                <join-column name="CENTRE_COUT_ID" referenced-column-name="ID"/>
            </inverse-join-columns>
        </join-table>
    </many-to-many>
    <field name="heuresServiceReferentiel" type="float" column="SERVICE_REFERENTIEL" precision="126" scale="0" nullable="false"/>
    <field name="heuresComplReferentiel" type="float" column="HEURES_COMPL_REFERENTIEL" precision="126" scale="0" nullable="false"/>
    <field name="total" type="float" column="TOTAL" precision="126" scale="0" nullable="false"/>
  </entity>
</doctrine-mapping>