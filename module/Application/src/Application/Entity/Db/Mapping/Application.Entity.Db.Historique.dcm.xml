<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Historique" table="HISTORIQUE">
    <indexes>
      <index name="IDX_E620C8D33CEB46C8" columns="CREATEUR"/>
      <index name="IDX_E620C8D368FB9ABE" columns="DESTRUCTEUR"/>
      <index name="IDX_E620C8D3D7603332" columns="MODIFICATEUR"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="debut" type="datetime" column="DEBUT" nullable="false"/>
    <field name="fin" type="datetime" column="FIN" nullable="true"/>
    <field name="modification" type="datetime" column="MODIFICATION" nullable="false"/>
    <many-to-one field="modificateur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="destructeur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="createur" target-entity="Application\Entity\Db\Utilisateur">
      <join-columns>
        <join-column name="CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
