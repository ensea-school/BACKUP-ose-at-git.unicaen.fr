<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Indicateur\Entity\Db\Indicateur" table="INDICATEUR">
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="numero" type="integer" column="NUMERO" nullable="false"/>
    <field name="ordre" type="integer" column="ORDRE" nullable="false"/>
    <field name="enabled" type="boolean" column="ENABLED" nullable="false"/>
    <field name="libelleSingulier" type="string" column="LIBELLE_SINGULIER" length="255" nullable="false"/>
    <field name="libellePluriel" type="string" column="LIBELLE_PLURIEL" length="255" nullable="false"/>
    <field name="message" type="string" column="MESSAGE" length="800" nullable="true"/>
    <field name="route" type="string" column="ROUTE" length="250" nullable="false"/>
    <field name="distinct" type="boolean" column="TEM_DISTINCT" nullable="false"/>
    <field name="notStructure" type="boolean" column="TEM_NOT_STRUCTURE" nullable="false"/>

    <many-to-one field="typeIndicateur" target-entity="Indicateur\Entity\Db\TypeIndicateur">
      <join-column name="TYPE_INDICATEUR_ID" referenced-column-name="ID"/>
    </many-to-one>
    <one-to-many field="notification" target-entity="Indicateur\Entity\Db\NotificationIndicateur" mapped-by="indicateur"/>
  </entity>
</doctrine-mapping>
