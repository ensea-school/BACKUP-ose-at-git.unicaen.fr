      <index name="IDX_2BC32905B3FC6B16" columns="ETABLISSEMENT_ID"/>
      <index name="IDX_2BC329057F29375B" columns="HISTORIQUE_ID"/>
      <index name="IDX_2BC32905E9B3446E" columns="SOURCE_ID"/>
      <index name="IDX_2BC32905C2443469" columns="TYPE_ID"/>
