<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Paiement\Entity\Db\TblPaiement" table="TBL_PAIEMENT">
    <unique-constraints>
      <unique-constraint name="tbl_paiement__un" columns="SERVICE_A_PAYER_ID,INTERVENANT_ID,MISE_EN_PAIEMENT_ID"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
      <sequence-generator sequence-name="TBL_PAIEMENT_ID_seq" allocation-size="1" initial-value="1"/>
    </id>
    <field name="heuresAPayer" type="float" column="HEURES_A_PAYER" precision="126" scale="0" nullable="false" />
    <field name="heuresAPayerPond" type="float" column="HEURES_A_PAYER_POND" precision="126" scale="0" nullable="false" />
    <field name="heuresDemandees" type="float" column="HEURES_DEMANDEES" precision="126" scale="0" nullable="false" />
    <field name="heuresPayees" type="float" column="HEURES_PAYEES" precision="126" scale="0" nullable="false" />
    <many-to-one field="formuleResultatService" target-entity="Application\Entity\Db\FormuleResultatService">
      <join-columns>
        <join-column name="FORMULE_RES_SERVICE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="formuleResultatServiceReferentiel" target-entity="Application\Entity\Db\FormuleResultatServiceReferentiel">
        <join-column name="FORMULE_RES_SERVICE_REF_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="mission" target-entity="Mission\Entity\Db\Mission">
      <join-column name="MISSIOn_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="structure" target-entity="Application\Entity\Db\Structure" fetch="LAZY">
        <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="periodePaiement" target-entity="Application\Entity\Db\Periode" fetch="LAZY">
        <join-column name="PERIODE_PAIEMENT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="miseEnPaiement" target-entity="Paiement\Entity\Db\MiseEnPaiement" fetch="LAZY">
        <join-column name="MISE_EN_PAIEMENT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant" fetch="LAZY">
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
    </many-to-one>
    <many-to-one field="annee" target-entity="Application\Entity\Db\Annee" fetch="LAZY">
        <join-column name="ANNEE_ID" referenced-column-name="ID"/>
    </many-to-one>
  </entity>
</doctrine-mapping>
