<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Contrat\Entity\Db\Contrat" table="CONTRAT">
        <indexes>
            <index name="IDX_5F71678959860CDC" columns="HISTO_CREATEUR_ID"/>
            <index name="IDX_5F7167897E9E2204" columns="HISTO_DESTRUCTEUR_ID"/>
            <index name="IDX_5F71678963F06898" columns="HISTO_MODIFICATEUR_ID"/>
            <index name="IDX_5F716789D81C5DAE" columns="TYPE_CONTRAT_ID"/>
        </indexes>
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="numeroAvenant" type="integer" column="NUMERO_AVENANT" nullable="false"/>
        <field name="dateRetourSigne" type="datetime" column="DATE_RETOUR_SIGNE" nullable="true"/>
        <field name="debutValidite" type="datetime" column="DEBUT_VALIDITE" nullable="true"/>
        <field name="finValidite" type="datetime" column="FIN_VALIDITE" nullable="true"/>
        <field name="dateEnvoiEmail" type="datetime" column="DATE_ENVOI_EMAIL" nullable="true"/>
        <field name="histoCreation" type="datetime" column="HISTO_CREATION" nullable="false"/>
        <field name="histoDestruction" type="datetime" column="HISTO_DESTRUCTION" nullable="true"/>
        <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
        <many-to-one field="typeContrat" target-entity="Contrat\Entity\Db\TypeContrat">
            <join-columns>
                <join-column name="TYPE_CONTRAT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="intervenant" target-entity="Intervenant\Entity\Db\Intervenant">
            <join-columns>
                <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\Utilisateur">
            <join-columns>
                <join-column name="HISTO_MODIFICATEUR_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\Utilisateur">
            <join-columns>
                <join-column name="HISTO_DESTRUCTEUR_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\Utilisateur">
            <join-columns>
                <join-column name="HISTO_CREATEUR_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="structure" target-entity="Lieu\Entity\Db\Structure">
            <join-columns>
                <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="validation" target-entity="Workflow\Entity\Db\Validation">
            <join-columns>
                <join-column name="VALIDATION_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-one field="contrat" target-entity="Contrat\Entity\Db\Contrat">
            <join-columns>
                <join-column name="CONTRAT_ID" referenced-column-name="ID"/>
            </join-columns>
        </many-to-one>
        <many-to-many field="fichier" target-entity="Application\Entity\Db\Fichier">
            <join-table name="CONTRAT_FICHIER">
                <join-columns>
                    <join-column name="CONTRAT_ID" referenced-column-name="ID"/>
                </join-columns>
                <inverse-join-columns>
                    <join-column name="FICHIER_ID" referenced-column-name="ID"/>
                </inverse-join-columns>
            </join-table>
        </many-to-many>
        <many-to-one field="processSignature" target-entity="UnicaenSignature\Entity\Db\Process" fetch="EAGER">
            <join-columns>
                <join-column name="PROCESS_SIGNATURE_ID"/>
            </join-columns>
        </many-to-one>
    </entity>
</doctrine-mapping>
