<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Enseignement\Entity\Db\TblValidationEnseignement" table="TBL_VALIDATION_ENSEIGNEMENT">
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
            <sequence-generator sequence-name="TBL_VALIDATION_ENS_ID_SEQ" allocation-size="1" initial-value="1"/>
        </id>

        <field name="autoValidation" type="boolean" column="AUTO_VALIDATION" nullable="false"/>
        <many-to-one field="annee" target-entity="Application\Entity\Db\Annee" fetch="LAZY">
            <join-column name="ANNEE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="intervenant" target-entity="Intervenant\Entity\Db\Intervenant" fetch="LAZY">
            <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="structure" target-entity="Lieu\Entity\Db\Structure" fetch="LAZY">
            <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="typeVolumeHoraire" target-entity="Service\Entity\Db\TypeVolumeHoraire" fetch="LAZY">
            <join-column name="TYPE_VOLUME_HORAIRE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="service" target-entity="Enseignement\Entity\Db\Service" fetch="LAZY">
            <join-column name="SERVICE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="volumeHoraire" target-entity="Enseignement\Entity\Db\VolumeHoraire" fetch="LAZY">
            <join-column name="VOLUME_HORAIRE_ID" referenced-column-name="ID"/>
        </many-to-one>
        <many-to-one field="validation" target-entity="Workflow\Entity\Db\Validation" fetch="LAZY">
            <join-column name="VALIDATION_ID" referenced-column-name="ID"/>
        </many-to-one>
    </entity>
</doctrine-mapping>
