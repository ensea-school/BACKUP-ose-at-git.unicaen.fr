<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\AdresseIntervenant" table="ADRESSE_INTERVENANT">
    <indexes>
      <index name="IDX_845CF54B7F29375B" columns="HISTORIQUE_ID"/>
      <index name="IDX_845CF54BE9B3446E" columns="SOURCE_ID"/>
      <index name="IDX_845CF54B78FF2BCB" columns="INTERVENANT_ID"/>
    </indexes>
    <unique-constraints>
      <unique-constraint name="adresse_intervenant_source_un" columns="SOURCE_CODE"/>
    </unique-constraints>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="batiment" type="string" column="BATIMENT" length="60" nullable="true"/>
    <field name="codePostal" type="string" column="CODE_POSTAL" length="15" nullable="true"/>
    <field name="localite" type="string" column="LOCALITE" length="60" nullable="true"/>
    <field name="mentionComplementaire" type="string" column="MENTION_COMPLEMENTAIRE" length="50" nullable="true"/>
    <field name="nomVoie" type="string" column="NOM_VOIE" length="60" nullable="true"/>
    <field name="noVoie" type="string" column="NO_VOIE" length="10" nullable="true"/>
    <field name="paysCodeInsee" type="string" column="PAYS_CODE_INSEE" length="3" nullable="false"/>
    <field name="paysLibelle" type="string" column="PAYS_LIBELLE" length="50" nullable="false"/>
    <field name="principale" type="boolean" column="PRINCIPALE" nullable="false"/>
    <field name="sourceCode" type="string" column="SOURCE_CODE" length="20" nullable="true"/>
    <field name="telDomicile" type="string" column="TEL_DOMICILE" length="25" nullable="true"/>
    <field name="ville" type="string" column="VILLE" length="60" nullable="true"/>
    <many-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant">
      <join-columns>
        <join-column name="INTERVENANT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="source" target-entity="Application\Entity\Db\Source">
      <join-columns>
        <join-column name="SOURCE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="historique" target-entity="Application\Entity\Db\Historique">
      <join-columns>
        <join-column name="HISTORIQUE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
