<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\FormuleIntervenant" table="V_FORMULE_INTERVENANT" read-only="true">
    <id name="id" type="integer" column="ID"></id>
    <many-to-one field="structure" target-entity="Application\Entity\Db\Structure">
      <join-columns>
        <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <field name="heuresServiceStatutaire" type="float" column="HEURES_SERVICE_STATUTAIRE" precision="126" scale="0" nullable="false"/>
    <one-to-one field="intervenant" target-entity="Application\Entity\Db\Intervenant">
      <join-columns>
        <join-column name="ID" referenced-column-name="ID"/>
      </join-columns>
    </one-to-one>
    <field name="depassementServiceDuSansHC" type="boolean" column="DEPASSEMENT_SERVICE_DU_SANS_HC" nullable="false" />
    <one-to-many field="formuleServiceModifie" target-entity="Application\Entity\Db\FormuleServiceModifie" mapped-by="formuleIntervenant" />
    <one-to-many field="formuleService" target-entity="Application\Entity\Db\FormuleService" mapped-by="intervenant" />
    <one-to-many field="formuleServiceReferentiel" target-entity="Application\Entity\Db\FormuleServiceReferentiel" mapped-by="formuleIntervenant" />
  </entity>
</doctrine-mapping>