<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Agrement" table="AGREMENT">
    <indexes>
      <index name="IDX_CD3F1085884B0F7B" columns="STRUCTURE_ID"/>
      <index name="IDX_CD3F1085AA401F5C" columns="ANNEE_ID"/>
      <index name="IDX_CD3F108581526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_CD3F1085F791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_CD3F1085862159D1" columns="HISTO_MODIFICATEUR"/>
      <index name="IDX_CD3F108520B9A217" columns="INTERVENANT_EXTERIEUR_ID"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="intervenantId" type="integer" column="INTERVENANT_ID" nullable="false"/>
    <field name="date" type="datetime" column="date" nullable="false"/>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenantExterieur" target-entity="Application\Entity\Db\IntervenantExterieur">
      <join-columns>
        <join-column name="INTERVENANT_EXTERIEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="annee" target-entity="Application\Entity\Db\Annee">
      <join-columns>
        <join-column name="ANNEE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="structure" target-entity="Application\Entity\Db\Structure">
      <join-columns>
        <join-column name="STRUCTURE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
