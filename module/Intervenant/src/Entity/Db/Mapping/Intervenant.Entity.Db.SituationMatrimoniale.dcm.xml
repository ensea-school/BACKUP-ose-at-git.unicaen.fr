<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping"
                  xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
    <entity name="Intervenant\Entity\Db\SituationMatrimoniale" table="SITUATION_MATRIMONIALE">
        <unique-constraints>
            <unique-constraint name="code_un" columns="CODE"/>
        </unique-constraints>
        <id name="id" type="integer" column="ID">
            <generator strategy="SEQUENCE"/>
        </id>
        <field name="libelle" type="string" column="LIBELLE" length="255" nullable="false"/>
        <field name="code" type="string" column="CODE" length="10" nullable="false"/>
    </entity>
</doctrine-mapping>
