<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\CompteBancaire" table="COMPTE_BANCAIRE">
    <indexes>
      <index name="cbie_fk" columns="INTERVENANT_EXTERIEUR_ID"/>
      <index name="IDX_B1A0E7DE81526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_B1A0E7DEF791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_B1A0E7DE862159D1" columns="HISTO_MODIFICATEUR"/>
    </indexes>
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="banqueBic" type="string" column="BANQUE_BIC" length="4" nullable="true"/>
    <field name="banqueId" type="string" column="BANQUE_ID" length="5" nullable="false"/>
    <field name="branche" type="string" column="BRANCHE" length="2" nullable="true"/>
    <field name="cleRib" type="string" column="CLE_RIB" length="2" nullable="false"/>
    <field name="compte" type="string" column="COMPTE" length="11" nullable="false"/>
    <field name="emplacement" type="string" column="EMPLACEMENT" length="2" nullable="true"/>
    <field name="guichet" type="string" column="GUICHET" length="5" nullable="false"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="iban2" type="string" column="IBAN_2" length="4" nullable="true"/>
    <field name="iban3" type="string" column="IBAN_3" length="4" nullable="true"/>
    <field name="iban4" type="string" column="IBAN_4" length="4" nullable="true"/>
    <field name="iban5" type="string" column="IBAN_5" length="4" nullable="true"/>
    <field name="iban6" type="string" column="IBAN_6" length="4" nullable="true"/>
    <field name="iban7" type="string" column="IBAN_7" length="4" nullable="true"/>
    <field name="ibanFin" type="string" column="IBAN_FIN" length="7" nullable="true"/>
    <field name="ibanIso" type="string" column="IBAN_ISO" length="4" nullable="true"/>
    <field name="paysIso" type="string" column="PAYS_ISO" length="2" nullable="true"/>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="intervenantExterieur" target-entity="Application\Entity\Db\IntervenantExterieur">
      <join-columns>
        <join-column name="INTERVENANT_EXTERIEUR_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
