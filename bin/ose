#!/usr/bin/env php
<?php

spl_autoload_register(function ($class) {
    $dir = dirname(__DIR__) . '/admin/src/';
    $class = $dir . str_replace('\\', '/', $class) . '.php';

    require_once $class;
});

$c  = new Console();
$oa = new OseAdmin($c);
$oa->version = $oa->currentVersion();
$oa->oldVersion = $oa->version;

    $c->printMainTitle("OSE", 15);

$oa->run($c->getArg(1));

$c->println('Traitement termin√©.', $c::BG_YELLOW);