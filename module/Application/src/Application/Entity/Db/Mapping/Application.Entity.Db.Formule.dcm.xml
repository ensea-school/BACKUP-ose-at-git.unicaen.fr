<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Formule" table="FORMULE">
    <id name="id" type="integer" column="ID">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="libelle" type="string" column="LIBELLE" length="100" nullable="false"/>
    <field name="packageName" type="string" column="PACKAGE_NAME" length="30" nullable="false"/>
    <field name="procedureName" type="string" column="PROCEDURE_NAME" length="30" nullable="false"/>
  </entity>
</doctrine-mapping>
