<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\AdresseIntervenant" table="ADRESSE_INTERVENANT">
    <indexes>
      <index name="IDX_845CF54B81526632" columns="HISTO_CREATEUR"/>
      <index name="IDX_845CF54BF791438F" columns="HISTO_DESTRUCTEUR"/>
      <index name="IDX_845CF54B862159D1" columns="HISTO_MODIFICATEUR"/>
      <index name="IDX_845CF54B54222575" columns="BIS_TER_ID"/>
