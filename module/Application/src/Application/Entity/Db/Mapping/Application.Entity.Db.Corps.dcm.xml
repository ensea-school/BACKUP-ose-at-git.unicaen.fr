<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Corps" table="CORPS">
    <id name="id" type="string" column="ID" length="4">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="libelleCourt" type="string" column="LIBELLE_COURT" length="4" nullable="false"/>
    <field name="libelleLong" type="string" column="LIBELLE_LONG" length="40" nullable="false"/>
    <field name="sourceCode" type="string" column="SOURCE_CODE" length="15" nullable="false"/>
    <many-to-one field="source" target-entity="Application\Entity\Db\Source">
      <join-columns>
        <join-column name="SOURCE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoModificateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_MODIFICATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoCreateur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_CREATEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="histoDestructeur" target-entity="Application\Entity\Db\User">
      <join-columns>
        <join-column name="HISTO_DESTRUCTEUR" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
