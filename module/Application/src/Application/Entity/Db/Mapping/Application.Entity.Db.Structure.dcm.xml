<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Application\Entity\Db\Structure" table="STRUCTURE">
    <indexes>
      <index name="IDX_2BC32905F1A6E54F" columns="PARENTE_ID"/>
      <index name="IDX_2BC32905B3FC6B16" columns="ETABLISSEMENT_ID"/>
      <index name="IDX_2BC32905C2443469" columns="TYPE_ID"/>
    </indexes>
    <id name="id" type="string" column="ID" length="10">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="histoCreateur" type="integer" column="HISTO_CREATEUR" nullable="false"/>
    <field name="histoDebut" type="datetime" column="HISTO_DEBUT" nullable="false"/>
    <field name="histoDestructeur" type="integer" column="HISTO_DESTRUCTEUR" nullable="true"/>
    <field name="histoFin" type="datetime" column="HISTO_FIN" nullable="true"/>
    <field name="histoModificateur" type="integer" column="HISTO_MODIFICATEUR" nullable="false"/>
    <field name="histoModification" type="datetime" column="HISTO_MODIFICATION" nullable="false"/>
    <field name="libelleCourt" type="string" column="LIBELLE_COURT" length="25" nullable="false"/>
    <field name="libelleLong" type="string" column="LIBELLE_LONG" length="60" nullable="false"/>
    <many-to-one field="type" target-entity="Application\Entity\Db\TypeStructure">
      <join-columns>
        <join-column name="TYPE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="etablissement" target-entity="Application\Entity\Db\Etablissement">
      <join-columns>
        <join-column name="ETABLISSEMENT_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="parente" target-entity="Application\Entity\Db\Structure">
      <join-columns>
        <join-column name="PARENTE_ID" referenced-column-name="ID"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
